# GNU Make workspace makefile autogenerated by Premake

.NOTPARALLEL:

ifndef config
  config=debug_x64
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug_x64)
  iv_config = debug_x64
endif
ifeq ($(config),release_x64)
  iv_config = release_x64
endif

PROJECTS := iv

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

iv:
ifneq (,$(iv_config))
	@echo "==== Building iv ($(iv_config)) ===="
	@${MAKE} --no-print-directory -C . -f iv.make config=$(iv_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f iv.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug_x64"
	@echo "  release_x64"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   iv"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"